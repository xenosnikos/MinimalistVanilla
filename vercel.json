{
  "version": 2,
  "buildCommand": "npm install && npx vite build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "framework": null,
  "routes": [
    {
      "src": "/new.html",
      "dest": "/new.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
} 